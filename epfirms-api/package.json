    "@types/express": "^4.17.12",
    "@types/redis": "^2.8.32",
    "@types/winston": "^2.4.4",
    "aws-sdk": "^2.957.0",
    "axios": "^0.21.4",
    "bcrypt": "^5.0.1",
    "chai-http": "^4.3.0",
    "cors": "^2.8.5",
    "dotenv": "^10.0.0",
    "express": "^4.17.1",
    "express-validator": "^6.12.0",
    "helmet": "^4.6.0",
    "jsonwebtoken": "^8.5.1",
    "mailgun-js": "^0.22.0",
    "module-alias": "^2.2.2",
    "mysql2": "^2.2.5",
    "passport": "^0.4.1",
    "passport-http-bearer": "^1.0.1",
    "redis": "^3.1.2",
    "sequelize": "^6.6.2",
